<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<GridPane styleClass="header" fx:controller="familyfeud.Controller_endScreen"
          xmlns:fx="http://javafx.com/fxml" alignment="CENTER" hgap="10" vgap="10" style="-fx-background-color:purple" gridLinesVisible="true">
    <columnConstraints>
        <ColumnConstraints percentWidth="50.0"/>
        <ColumnConstraints percentWidth="50.0"/>
    </columnConstraints>
    <GridPane GridPane.rowIndex="0" GridPane.columnIndex="0" alignment="CENTER" hgap="10" vgap="10">
        <Label  style="-fx-font-size:20; -fx-text-fill: white;" text="Winning Team!!" GridPane.rowIndex="1" GridPane.columnIndex="3" />
        <Label fx:id="winnerTeam" style="-fx-font-size:20; -fx-text-fill: white;" text="00" GridPane.rowIndex="3" GridPane.columnIndex="3" />
        <Label fx:id="teamPlayers" style="-fx-font-size:20; -fx-text-fill: white;" text="00" GridPane.rowIndex="4" GridPane.columnIndex="3" />
        <Label  style="-fx-font-size:20; -fx-text-fill: white;" text="Select Game to play" GridPane.rowIndex="6" GridPane.columnIndex="3" />
        <Button text="Return to Start" GridPane.rowIndex="10" GridPane.columnIndex="3" onMouseClicked="#startNewGame" />
    </GridPane >

</GridPane>